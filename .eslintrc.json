{
    "extends": "airbnb",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "eqeqeq": "error",
        "no-trailing-spaces": "error",
        "object-curly-spacing": [
            "error",
            "always"
        ],
        "arrow-spacing": [
            "error",
            {
                "before": true,
                "after": true
            }
        ],
        "no-console": 0,
        "linebreak-style": 0,
        "no-underscore-dangle":  0
    }
}